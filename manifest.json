{
    "manifest_version": 2,
    "name": "Weblab++",
    "description": "You now have to wait way to long for the preview to actually render.",
    "version": "1.0",
    "page_action": {
        "default_icon": "weblabpp.png",
        "default_title": "Weblab++"
    },
    "icons": { 
        "16": "weblabpp16.png",
        "48": "weblabpp48.png",
       "128": "weblabpp128.png" 
    },
    "content_scripts": [
        {
            "matches": ["https://weblab.tudelft.nl/*"],
            "js": ["weblabpp.js"]
        }
    ]
}